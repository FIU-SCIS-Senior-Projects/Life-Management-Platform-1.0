
@model List<LifeManagement.Models.Coach>
@{
    var common = new LifeManagement.Models.Common();
}
<link href="~/Content/coaches.css" rel="stylesheet" />


        <table class="table">
            @{
                foreach (var m in Model)
                {
                    <tr>
                        <td>
                            
                                <table class="table coach-table">
                                    <tr class="no-border">
                                        <td class="coachimgtd no-border" rowspan="5"><img class="coachimg" src="@common.SignatureImageStr64(m.Avatar, m.AvatarMime)" /></td>
                                        <td class="no-border">
                                            @(@m.FirstName + " " + m.LastName)

                                        </td>
                                    </tr>
                                    <tr class="no-border">
                                        <td class="no-border">
                                            @{
                                                for (int i = 0; i < m.ReviewScore; i++)
                                                {
                                                    <div class="stardiv">
                                                    </div>
                                                }
                                            }

                                        </td>
                                    </tr>
                                    <tr class="no-border">
                                        <td class="no-border">@m.Skills</td>
                                    </tr>
                                    <tr class="no-border">
                                        <td class="no-border">@m.Email</td>
                                    </tr>
                                    <tr class="no-border">
                                        <td class="no-border">
                                            <a class="lmmodal" modal-width="700px" href="#" url="/Coaches/CoachDetailsPage/@m.Id">Details</a> |
                                            <a href="@Url.Action("SeeReviews","Coaches",new {id=m.Id})">Reviews</a>
                                         </td>

                                    </tr>

                                    <tr class="no-border"><td class="no-border" colspan="2"><hr /></td></tr>
                                </table>
                            
                        </td>
                    </tr>
                                                }
            }

        </table>




<link href="~/Content/coaches.css" rel="stylesheet" />


<script>
    $(document).ready(function () {
        setpopups();
    })
</script>