@Scripts.Render("~/bundles/bootstrap")

<link href="~/Content/dashboard_css/bootstrap.min.css" rel="stylesheet">
<link href="~/Content/dashboard_css/datepicker3.css" rel="stylesheet">
<link href="~/Content/dashboard_css/styles.css" rel="stylesheet">
<script src="~/Scripts/dashboard_js/lumino.glyphs.js"></script>
<link href="~/Content/coaches.css" rel="stylesheet" />
<!--[if lt IE 9]>
<script src="js/html5shiv.js"></script>
