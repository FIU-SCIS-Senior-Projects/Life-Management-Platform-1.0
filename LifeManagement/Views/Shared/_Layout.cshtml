
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <script src="~/Scripts/Site.js"></script>

    @Scripts.Render("~/bundles/jquery")
  
    @RenderSection("scripts", required: false)
    <link href="~/Content/dashboard_css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/dashboard_css/datepicker3.css" rel="stylesheet">
    <link href="~/Content/dashboard_css/styles.css" rel="stylesheet">
    <script src="~/Scripts/dashboard_js/lumino.glyphs.js"></script>

    <!--[if lt IE 9]>
    <script src="js/html5shiv.js"></script>
    <script src="js/respond.min.js"></script>
    <![endif]-->

    <script src="~/Scripts/dashboard_js/chart.min.js"></script>
    <script src="~/Scripts/dashboard_js/chart-data.js"></script>
    <script src="~/Scripts/dashboard_js/easypiechart.js"></script>
    <script src="~/Scripts/dashboard_js/easypiechart-data.js"></script>
    <script src="~/Scripts/dashboard_js/bootstrap-datepicker.js"></script>
   
    @if (IsSectionDefined("AddToHead"))
    {
        @RenderSection("AddToHead", required: false)
    }

    @RenderSection("AddToHeadAnotherWay", required: false)
  
</head>
<body>
    

    @if (IsSectionDefined("bgdiv"))
    {
        @RenderSection("bgdiv");
    }
    else
    {
        <bgdiv>
            <div class="bgdiv"></div>
        </bgdiv>
    }



<div class="modal fade" id="basicModal" tabindex="-1" role="dialog" aria-labelledby="basicModal" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content" id="modalcontent">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Close</button>

            </div>
            <div class="modal-body" id="modalbodydiv">
                <div id="myDialogContainer">

                </div>

            </div>

        </div>
    </div>
</div>

    <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#sidebar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#"><span>Managed</span>Life</a>
                <ul class="user-menu">
                   @{ Html.RenderPartial("userMenu"); }
                  
                </ul>
            </div>

        </div><!-- /.container-fluid -->
    </nav>
<div class="container-full body-content">
    @RenderBody()
    <hr/>
    <footer>
        <p>&copy; @DateTime.Now.Year - managedLife</p>
    </footer>
</div>

</body>
</html>
